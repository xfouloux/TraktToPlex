{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TraktConfig": { // create trakt api app at https://trakt.tv/oauth/applications/new
    // Only important field is Redirect uri, which should be https://localhost:44364/Home/TraktReturn (modify hostname and port for your own needs. Localhost works perfectly fine for development)
    "ClientId": "$TRAKT_CLIENTID",
    "ClientSecret": "$TRAKT_CLIENT_SECRET"
  },
  "PlexConfig": {
    "ClientSecret": "$PLEX_CLIENT_SECRET" // Just make your own, for example generate a random guid
  } 
}
